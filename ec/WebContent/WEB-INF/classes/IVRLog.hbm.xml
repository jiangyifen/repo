<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.jiangyifen.ec.dao.IVRLog" table="ec_ivr_log">

		<id name="id">
			<column name="id" not-null="true" />
			<generator class="identity" />
		</id>

		<property name="date" not-null="true" />
		<property name="uniqueid" not-null="true" length="32" />
		<property name="context" not-null="true" length="32" />
		<property name="exten" not-null="true" length="32" />
		<property name="priority" not-null="true" length="8" />
		<property name="calleridnum" not-null="true" length="32" />
		<property name="channel" not-null="true" length="32" />
		<property name="node" not-null="true" length="128" />


	</class>
</hibernate-mapping>