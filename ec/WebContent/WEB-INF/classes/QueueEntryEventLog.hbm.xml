<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.jiangyifen.ec.dao.QueueEntryEventLog" table="ec_queue_entry_event_log">
        <id name="id">
            <column name="id" not-null="true"/>
            <generator class="identity"/>
        </id>
        <property name="queue" not-null="true"/>
        <property name="dateReceived" not-null="true"/>
        <property name="callerid" not-null="true"/>
        <property name="calleridname" not-null="true"/>
        <property name="channel" not-null="true"/>
        <property name="position" not-null="true"/>
        <property name="wait" not-null="true"/>
        
	</class>
</hibernate-mapping>