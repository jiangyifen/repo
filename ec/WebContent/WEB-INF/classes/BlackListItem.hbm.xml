<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.jiangyifen.ec.dao.BlackListItem" table="ec_black_list_item">
	
		<id name="id">
			<column name="id" not-null="true"/>
			<generator class="identity"/>
		</id>
        <property name="phoneNumber" not-null="true" length="32"/>
        <property name="type" not-null="true" length="8"/>
        <property name="remark" not-null="false" length="256"/>

	</class>
</hibernate-mapping>