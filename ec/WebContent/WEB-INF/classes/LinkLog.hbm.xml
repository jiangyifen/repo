<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.jiangyifen.ec.dao.LinkLog" table="ec_link_log">

		<id name="id">
			<column name="id" not-null="true" />
			<generator class="identity" />
		</id>

		<property name="date" not-null="true" />
		<property name="channel" not-null="true" length="32" />
		<property name="bridgedChannel" not-null="true" length="32" />
		<property name="uniqueid" not-null="true" length="32" />
		<property name="bridgedUniqueid" not-null="true" length="32" />
		<property name="exten" not-null="true" length="32" />
		<property name="callerid" not-null="true" length="32" />
		<property name="username" not-null="true" length="32"></property>
		<property name="name" not-null="true" length="16"></property>
		<property name="action" not-null="true" length="16"></property>


	</class>
</hibernate-mapping>