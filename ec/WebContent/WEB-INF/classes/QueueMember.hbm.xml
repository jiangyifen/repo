<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.jiangyifen.ec.dao.QueueMember" table="queue_member_table">
		<composite-id>
			<key-property name="queueName" column="queue_name" />
			<key-property name="iface" column="interface" />
		</composite-id>
		<property name="penalty" column="penalty" />
	</class>

</hibernate-mapping>