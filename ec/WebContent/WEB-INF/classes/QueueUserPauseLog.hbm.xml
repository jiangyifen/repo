<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.jiangyifen.ec.dao.QueueUserPauseLog" table="ec_queue_user_pause_log">
        <id name="id">
            <column name="id" not-null="true"/>
            <generator class="identity"/>
        </id>
        <property name="username" not-null="true" length="32"/>
        <property name="sipname" not-null="true" length="32"/>
        <property name="pauseDate" not-null="false"/>
        <property name="unpauseDate" not-null="false"/>
	</class>
</hibernate-mapping>